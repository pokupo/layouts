.pkp_html .pkp_body
    .pkp-wrapper {
        background: #333333;
    }
    #pkp-container {
        background: #333333;
        border: 2px solid #efece7;
    }
    #pkp-container .mCSB_scrollTools .mCSB_dragger {
        background: #ffa800;
    }
    #pkp-container input:focus {
        border-color: #ffa800;
    }
.pkp_html .pkp_body #pkp-container
    .pkp-payment-method__item:not(.pkp-payment-method__item_disabled):hover {
        border-color: #ffa800;
    }
    .pkp-print {
        color: #ccc;
    }
    .pkp-print:before {
        background-image: url('data:image/png;base64,');
    }
    .pkp-payment-info td:first-child {
        color: #ccc;
    }
    .pkp-payment-info td:last-child {
        color: #ccc;
    }
    .pkp-input-radio:checked + .pkp-span-radio {
        background-image: url('data:image/png;base64,');
    }
    .pkp-input-checkbox:checked + .pkp-span-checkbox {
        background-image: url('data:image/png;base64,');
    }
    .pkp-payment-method__pay {
        background: #ffa800;
    }
    .pkp-payment-method__pay:active {
        background: #b27500;
    }
    .pkp-btn_back {
        background: #333333;
        border: 2px solid #efece7;
        color: #fff;
    }
    .pkp-btn_back:before {
        background-image: url('data:image/png;base64,');
    }
    .pkp-btn_back:hover {
        background: #efece7;
        color: #000;
        border-color: #efece7;
    }
    .pkp-btn_back:hover:before {
        background-image: url('data:image/png;base64,');
    }
    .pkp-btn_back:active:before {
        background-image: url('data:image/png;base64,');
    }
    .pkp-btn_back:active {
        background: #cbc9c5;
        color: #000;
        border-color: #cbc9c5;
    }
    .pkp-btn_get-receipt, .pkp-btn_upload {
        background: #ffa800;
    }
    .pkp-btn_get-receipt:hover, .pkp-btn_upload:hover {
        background: #d98f00;
    }
    .pkp-btn_get-receipt:active, .pkp-btn_upload:active {
        background: #b27500;
    }
    .pkp-title, .pkp-strong {
        color: #fff;
    }
    .pkp-span {
        color: #ccc;
    }
    .pkp-shopping-list__title,
    .pkp-contributors-intruct__list-item h5,
    .pkp-contributors-intruct__list-article,
    .pkp-contributors-intruct__list-item p {
        color: #fff;
    }
    .pkp-shopping-list__info, 
    .pkp-upload__info,
    .pkp-span-checkbox,
    .pkp-span-radio {
        color: #ccc;
    }

